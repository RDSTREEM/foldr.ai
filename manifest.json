{
  "manifest_version": 3,
  "name": "Foldr",
  "version": "0.1",
  "description": "Organize ChatGPT chats by folders.",
  "permissions": ["storage"],
  "host_permissions": ["https://chatgpt.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
